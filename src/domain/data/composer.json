{
	"name": "{owner}/yii2-{name}",
	"type": "yii2-extension",
	"keywords": ["yii2","{name}"],
	"license": "MIT",
	"authors": [
		{
			"name": "{author}",
			"email": "{email}"
		}
	],
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"{owner}\\{name}\\": "src"
			"{owner}\\{name}\\tests\\": "tests"
		}
	},
	"require": {
		"yiisoft/yii2": "*",
		"php": ">=5.3.0"
	}
}
